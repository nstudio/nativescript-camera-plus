{
  "name": "@nstudio/nativescript-camera-plus",
  "version": "3.1.1",
  "description": "An advanced, embeddable camera for NativeScript.",
  "main": "camera-plus",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "5.0.0",
      "ios": "5.0.0"
    }
  },
  "scripts": {
    "build.native": "node scripts/build-native.js",
    "setup": "npm i && ts-patch install",
    "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\" --exclude \"**/platforms/**\" --exclude \"**/typings/**\""
  },
  "lint-staged": {
    "**/*.{js, css, ts, json, scss, html, xml, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nstudio/nativescript-camera-plus.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS",
    "Camera",
    "Camera Plus"
  ],
  "author": {
    "name": "Nathan Walker",
    "email": "nwalker@nstudio.io"
  },
  "contributors": [
    {
      "name": "Brad Martin",
      "email": "bmartin@nstudio.io"
    },
    {
      "name": "Nathanael Anderson",
      "email": "nanderson@nstudio.io"
    },
    {
      "name": "Dick Smith",
      "url": "https://github.com/DickSmith"
    },
    {
      "name": "Shawn Pavel",
      "url": "https://github.com/ShawnPavel"
    },
    {
      "name": "Osei Fortune",
      "email": "osei@nstudio.io"
    },
    {
      "name": "Alex Miller",
      "url": "https://github.com/codex-"
    }
  ],
  "bugs": {
    "url": "https://github.com/nstudio/nativescript-camera-plus/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/nstudio/nativescript-camera-plus",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "nativescript-permissions": ">=1.3.11"
  },
  "devDependencies": {
    "@nativescript/core": "~7.0.13",
    "@nativescript/types": "~7.0.4",
    "@nativescript/webpack": "~3.0.8",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "nativescript-permissions": "^1.3.11",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "ts-node": "^9.1.1",
    "ts-patch": "^1.3.1",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7"
  }
}
